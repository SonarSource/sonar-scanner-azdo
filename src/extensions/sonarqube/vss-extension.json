{
  "manifestVersion": 1,
  "id": "sonarqube",
  "name": "SonarQube",
  "version": "7.0.4",
  "branding": {
    "color": "rgb(67, 157, 210)",
    "theme": "dark"
  },
  "publisher": "sonarsource",
  "homepage": "https://www.sonarsource.com",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services.Cloud"
    },
    {
      "id": "Microsoft.TeamFoundation.Server",
      "version": "[15.2,)"
    }
  ],
  "description": "Detect bugs, vulnerabilities and code smells across project branches and pull requests.",
  "public": true,
  "categories": ["Build and release"],
  "icons": {
    "default": "extension-icon.png"
  },
  "tags": [
    "build",
    "ci",
    "continuous integration",
    "code quality",
    "code analysis",
    "SAST",
    "security"
  ],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "license-terms.md"
    }
  },
  "links": {
    "home": {
      "uri": "https://www.sonarsource.com"
    },
    "getstarted": {
      "uri": "https://docs.sonarsource.com/sonarqube/latest/analyzing-source-code/scanners/sonarqube-extension-for-azure-devops/"
    },
    "support": {
      "uri": "https://community.sonarsource.com/"
    },
    "privacypolicy": {
      "uri": "https://www.sonarsource.com/company/privacy/"
    }
  },
  "CustomerQnASupport": {
    "enableqna": "true",
    "url": "https://community.sonarsource.com/"
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/SonarSource/sonar-scanner-azdo"
  },
  "files": [
    {
      "path": "tasks"
    },
    {
      "path": "img",
      "addressable": true
    },
    {
      "path": "icons",
      "addressable": true
    }
  ],
  "scopes": ["vso.build", "vso.code", "vso.test"],
  "contributions": [
    {
      "id": "15B84CA1-B62F-4A2A-A403-89B77A063157",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "tasks/SonarQubePrepare"
      }
    },
    {
      "id": "6D01813A-9589-4B15-8491-8164AEB38055",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "tasks/SonarQubeAnalyze"
      }
    },
    {
      "id": "291ed61f-1ee4-45d3-b1b0-bf822d9095ef",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "tasks/SonarQubePublish"
      }
    },
    {
      "id": "EDC775DD-1A82-4ADD-A073-7B5813E87202",
      "description": "Service Endpoint type for SonarQube Connections",
      "type": "ms.vss-endpoint.service-endpoint-type",
      "targets": ["ms.vss-endpoint.endpoint-types"],
      "properties": {
        "name": "sonarqube",
        "displayName": "SonarQube",
        "icon": "icons/SonarQube_endpoint.png",
        "url": {
          "displayName": "Server Url",
          "helpText": "Url for the SonarQube Server to connect to."
        },
        "authenticationSchemes": [
          {
            "type": "ms.vss-endpoint.endpoint-auth-scheme-basic",
            "inputDescriptors": [
              {
                "id": "username",
                "name": "Token",
                "description": "Authentication Token generated through SonarQube (go to My Account > Security > Generate Tokens)",
                "inputMode": "passwordbox",
                "isConfidential": true,
                "validation": {
                  "isRequired": true,
                  "dataType": "string"
                }
              },
              {
                "id": "password",
                "name": "Password",
                "description": "Password",
                "inputMode": "passwordbox",
                "isConfidential": true,
                "validation": {
                  "isRequired": false,
                  "dataType": "string"
                },
                "values": {
                  "inputId": "passwordInput",
                  "defaultValue": "",
                  "isDisabled": true
                }
              }
            ]
          }
        ],
        "helpMarkDown": ""
      }
    }
  ]
}
