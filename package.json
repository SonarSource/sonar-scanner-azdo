{
  "name": "sonar-scanner-azdo",
  "version": "0.0.2-SNAPSHOT",
  "repository": "SonarSource/sonar-scanner-azdo",
  "license": "LGPL-3.0",
  "scripts": {
    "postinstall": "find src/common -mindepth 1 -maxdepth 1 -type d -exec sh -c 'npm install --prefix \"$0\"' {} \\;",
    "build": "BUILD_MODE=production gulp",
    "test-build": "gulp",
    "clean": "gulp clean",
    "upload": "gulp upload",
    "promote": "gulp promote",
    "sonarqube": "gulp sonarqube:scan",
    "format": "prettier --write --list-different \"src/**/*.{js,ts}\"",
    "format-check": "prettier --list-different \"src/**/*.{js,ts}\"",
    "lint": "eslint --ext js,ts --quiet \"src/**/*.{js,ts}\"",
    "lint-report": "eslint --ext js,ts -f json -o eslint-report.json \"src/**/*.{js,ts}\"",
    "test": "find src/common -mindepth 1 -maxdepth 1 -type d -exec sh -c 'npm run test --prefix \"$0\"' {} \\;",
    "test-ci": "find src/common -mindepth 1 -maxdepth 1 -type d -exec sh -c 'npm run test-ci --prefix \"$0\"' {} \\;",
    "ts-check": "gulp build:typescript",
    "validate": "npm run lint && npm run ts-check && npm run format-check && npm run test",
    "validate-ci": "npm run lint-report && npm run ts-check && npm run format-check && npm run test-ci",
    "cyclonedx-run": "cyclonedx-node",
    "cyclonedx": "gulp cyclonedx"
  },
  "devDependencies": {
    "@cyclonedx/bom": "3.8.0",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "29.5.8",
    "@types/jest-when": "2.7.3",
    "@types/node": "14.14.31",
    "@types/yargs": "13.0.2",
    "@typescript-eslint/parser": "5.59.11",
    "dateformat": "3.0.3",
    "del": "3.0.0",
    "esbuild": "0.21.3",
    "eslint": "8.48.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-promise": "6.1.1",
    "fs-extra": "10.0.0",
    "globby": "7.1.1",
    "gulp": "4.0.2",
    "gulp-artifactory-upload": "1.4.0",
    "gulp-collect": "0.1.0",
    "gulp-decompress": "2.0.2",
    "gulp-download": "0.0.1",
    "gulp-exec": "5.0.0",
    "gulp-file": "0.4.0",
    "gulp-json-editor": "2.5.6",
    "gulp-rename": "1.2.2",
    "gulp-typescript": "4.0.2",
    "gulp-util": "3.0.8",
    "jest": "29.7.0",
    "jest-when": "3.5.2",
    "map-stream": "0.1.0",
    "merge-stream": "2.0.0",
    "needle": "3.2.0",
    "openpgp": "5.10.1",
    "prettier": "3.0.3",
    "sonarqube-scanner": "4.0.1",
    "tfx-cli": "0.16.0",
    "through2": "4.0.2",
    "ts-jest": "29.1.1",
    "typescript": "4.4.4",
    "vinyl": "2.2.1",
    "yargs": "17.7.0"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=8"
  }
}
